{
	"expo": {
		"name": "softruck",
		"slug": "softruck",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./app/assets/images/icon.png",
		"scheme": "softruck",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"edgeToEdgeEnabled": true
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./app/assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			],
			[
				"react-native-edge-to-edge",
				{
					"android": {
						"parentTheme": "Default",
						"enforceNavigationBarContrast": false
					}
				}
			],
			"expo-localization"
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
